<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layoutFamilyMembers"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".nammaapartmentsservices.digitalgate.mysweethome.MySweetHome">

    <include layout="@layout/activity_base" />

    <Button
        android:id="@+id/buttonAddFamilyMembers"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_margin="@dimen/button_margin"
        android:background="@color/nmBlack"
        android:text="@string/add_my_family_members"
        android:textAllCaps="false"
        android:textColor="@color/nmLightYellow"
        android:textSize="@dimen/button_default_size" />
</RelativeLayout>